oods to mealsitem in order to acces each item\r\n  console.log(meals);\r\n\r\n  const mealsList = meals\r\n    .slice(indexOfFirstPic, indexOfLastPic)\r\n    .map((meal) => (\r\n      <MealsItem\r\n        id={meal.id}\r\n        key={meal.id}\r\n        title={meal.title}\r\n        description={meal.description}\r\n        price={meal.price}\r\n      />\r\n    ));\r\n  return (\r\n    <div className=\"container\">\r\n      <div className={classes.containercard}>{mealsList}</div>\r\n\r\n      <Pagination\r\n        picturePerPage={picturePerPage}\r\n        totalPictures={meals.length}\r\n        paginate={paginate}\r\n      />\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default AvailableMeals;\r\n","C:\\Users\\S ONE\\Desktop\\udemy-workspace\\Order-Food-App\\src\\components\\layout\\HeaderCartButton.js",[],"C:\\Users\\S ONE\\Desktop\\udemy-workspace\\Order-Food-App\\src\\components\\meals\\MealsSummary.js",[],"C:\\Users\\S ONE\\Desktop\\udemy-workspace\\Order-Food-App\\src\\components\\UI\\Model.js",[],"C:\\Users\\S ONE\\Desktop\\udemy-workspace\\Order-Food-App\\src\\components\\UI\\Card.js",[],"C:\\Users\\S ONE\\Desktop\\udemy-workspace\\Order-Food-App\\src\\components\\Cart\\CartIcon.js",[],"C:\\Users\\S ONE\\Desktop\\udemy-workspace\\Order-Food-App\\src\\components\\meals\\mealsitem\\MealsItem.js",["93"],"import { useContext } from \"react\";\r\nimport MealItemForm from \"./MealItemForm\";\r\nimport \"./MealsItem.css\";\r\nimport CartContext from \"../../../store/Cart-Context\";\r\n\r\nconst MealsItem = (props) => {\r\n  const cartCtx = useContext(CartContext);\r\n\r\n  const price = `$${props.price}`;\r\n  const addToCartHandler = (amount) => {\r\n    cartCtx.addItem({\r\n      id: props.id,\r\n      title: props.title,\r\n      amount: amount,\r\n      price: props.price,\r\n    });\r\n  };\r\n  return (\r\n    // <li className=\"meal\">\r\n    //   <div classname=\"left\">\r\n    //     <h1>{props.title}</h1>\r\n\r\n    //     <div className=\"description\">{p